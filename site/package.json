{
  "name": "vue-sostatic",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build && purgecss --css dist\\lib\\bootstrap.min.css --content dist\\landing.html dist\\legal\\*.html --out dist\\lib\\ && echo 'Done'",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "core-js": "^2.6.5",
    "dotenv": "^8.0.0",
    "firebase": "^6.3.1",
    "is-valid-domain": "^0.0.11",
    "lodash.clonedeep": "^4.5.0",
    "moment": "^2.24.0",
    "moment-duration-format": "^2.3.2",
    "pixi-ease": "^2.3.0",
    "pixi.js": "^5.1.0",
    "url-parse": "^1.4.7",
    "vee-validate": "^2.2.13",
    "vue": "^2.6.10",
    "vue-highlightjs": "^1.3.3",
    "vue-recaptcha": "^1.2.0",
    "vue-router": "^3.0.7",
    "vue-svg-loader": "^0.12.0",
    "vuetify": "^2.0.0",
    "vuetify-confirm": "^0.2.6",
    "vuex": "^3.1.1",
    "zipcelx": "^1.6.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.9.0",
    "@vue/cli-plugin-eslint": "^3.9.0",
    "@vue/cli-service": "^3.9.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "sass": "^1.17.4",
    "sass-loader": "^7.1.0",
    "vue-cli-plugin-vuetify": "^0.6.1",
    "vue-template-compiler": "^2.6.10",
    "vuetify-loader": "^1.2.2",
    "purgecss": "^1.3.0",
    "eslint-plugin-vuetify": "^1.0.0-beta.3"
  },
  "babel": {
    "presets": [
      "@vue/app"
    ]
  },
  "eslintConfig": {
    "root": true,
    "plugins": [
      "vuetify"
    ],
    "rules": {
      "no-console": "off",
      "vue/max-attributes-per-line": [
        "error",
        {
          "singleline": 5,
          "multiline": {
            "max": 5,
            "allowFirstLine": true
          }
        }
      ]
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
